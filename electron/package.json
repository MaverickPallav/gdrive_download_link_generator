{
    "name": "streamlit-app",
    "version": "1.0.0",
    "description": "A native app for a Streamlit application using Electron.",
    "main": "main.js",
    "scripts": {
      "start": "electron .",
      "package-mac": "electron-packager . StreamlitApp --platform=darwin --arch=x64 --out=dist/",
      "package-win": "electron-packager . StreamlitApp --platform=win32 --arch=x64 --out=dist/"
    },
    "devDependencies": {
      "electron": "^26.2.0",
      "electron-packager": "^17.1.1"
    }
  }
  